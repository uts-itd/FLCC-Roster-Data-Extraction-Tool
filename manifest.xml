<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>abc0472d-d055-4f11-8835-75d4a3c565e5</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Lifetime Learner Experience Unit</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="FLCC Roster Data Extraction Tool"/>
  <Description DefaultValue="A tool to ecxtract roster data into a table."/>
  <IconUrl DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/assets/UTS_logo_web-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/assets/UTS_logo_web-64.png"/>
  <SupportUrl DefaultValue="https://github.com/uts-itd/FLCC-Roster-Data-Extraction-Tool/wiki"/>
  <AppDomains>
	  <AppDomain>https://uts-itd.github.io</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>

  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
		  <FunctionFile resid="Commands.Url"/>
		  <ExtensionPoint xsi:type="PrimaryCommandSurface">
			  <CustomTab id="FRDET.Tab1">

				  <Group id="FRDET.Group1">
					  <Label resid="FRDET.Group1.Label"/>
					  <Icon>
						  <bt:Image size="16" resid="Icon.16x16"/>
						  <bt:Image size="32" resid="Icon.32x32"/>
						  <bt:Image size="80" resid="Icon.80x80"/>
					  </Icon>
					  <Control xsi:type="Button" id="ExtractDataButton">
						  <Label resid="ExtractButton.Label"/>
						  <Supertip>
							  <Title resid="ExtractButton.Label"/>
							  <Description resid="ExtractButton.Tooltip"/>
						  </Supertip>
						  <Icon>
							  <bt:Image size="16" resid="Extract.16x16"/>
							  <bt:Image size="32" resid="Extract.32x32"/>
							  <bt:Image size="80" resid="Extract.80x80"/>
						  </Icon>
						  <Action xsi:type="ExecuteFunction">
							  <FunctionName>extractData</FunctionName>
						  </Action>
					  </Control>
				  </Group>

				  <Group id="FRDET.Group2">
					  <Label resid="FRDET.Group2.Label"/>
					  <Icon>
						  <bt:Image size="16" resid="Icon.16x16"/>
						  <bt:Image size="32" resid="Icon.32x32"/>
						  <bt:Image size="80" resid="Icon.80x80"/>
					  </Icon>
					  <Control xsi:type="Button" id="AboutButton">
						  <Label resid="AboutButton.Label"/>
						  <Supertip>
							  <Title resid="AboutButton.Label"/>
							  <Description resid="AboutButton.Tooltip"/>
						  </Supertip>
						  <Icon>
							  <bt:Image size="16" resid="About.Icon.16x16"/>
							  <bt:Image size="32" resid="About.Icon.32x32"/>
							  <bt:Image size="80" resid="About.Icon.80x80"/>
						  </Icon>
						  <Action xsi:type="ShowTaskpane">
							  <TaskpaneId>ButtonId1</TaskpaneId>
							  <SourceLocation resid="Taskpane.Url"/>
						  </Action>
					  </Control>
				  </Group>

				  <Label resid="FRDET.Tab1.Label" />

			  </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>

    <Resources>

      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/assets/UTS_logo_web-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/assets/UTS_logo_web-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/assets/UTS_logo_web-80.png"/>

        <bt:Image id="Extract.16x16" DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/assets/play_16x16.png"/>
        <bt:Image id="Extract.32x32" DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/assets/play_32x32.png"/>
        <bt:Image id="Extract.80x80" DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/assets/play_80x80.png"/>

        <bt:Image id="About.Icon.16x16" DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/assets/about_16.png"/>
        <bt:Image id="About.Icon.32x32" DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/assets/about_32.png"/>
        <bt:Image id="About.Icon.80x80" DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/assets/about_80.png"/>
      </bt:Images>

      <bt:Urls>
        <bt:Url id="Commands.Url" DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://uts-itd.github.io/FLCC-Roster-Data-Extraction-Tool/taskpane.html"/>
      </bt:Urls>

      <bt:ShortStrings>
        <bt:String id="FRDET.Group1.Label" DefaultValue="FRDET"/>
        <bt:String id="FRDET.Group2.Label" DefaultValue="Acknowledgements"/>
        <bt:String id="ExtractButton.Label" DefaultValue="Extract Data"/>
        <bt:String id="AboutButton.Label" DefaultValue="About"/>
        <bt:String id="FRDET.Tab1.Label" DefaultValue="FLCC Roster Data Extraction Tool"/>
      </bt:ShortStrings>

      <bt:LongStrings>
        <bt:String id="AboutButton.Tooltip" DefaultValue="About the FLCC Roster Data Extraction Tool"/>
        <bt:String id="ExtractButton.Tooltip" DefaultValue="Extract data from FLCC roster"/>
      </bt:LongStrings>

    </Resources>
  </VersionOverrides>
</OfficeApp>
